<%= form_for @listing do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :image_url %>
  <%= f.text_field :image_url %>

  <%= f.label :city %>
  <%= f.collection_select :city_id, City.order(:name), :id, :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :price_per_night %>
  <%= f.number_field :price_per_night, step: 0.01 %>





<% end %>
